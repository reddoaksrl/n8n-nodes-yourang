{
  "name": "n8n-nodes-yourang",
  "version": "0.5.1",
  "description": "n8n community node for Yourang.ai - 24/7 AI phone assistant for automated call handling, appointments, customer interactions, and event management",
  "keywords": [
    "n8n-community-node-package",
    "yourang",
    "ai-assistant",
    "phone-automation",
    "appointments",
    "call-handling",
    "customer-service",
    "events",
    "calendar"
  ],
  "license": "MIT",
  "homepage": "https://github.com/reddoaksrl/n8n-nodes-yourang#readme",
  "bugs": {
    "url": "https://github.com/reddoaksrl/n8n-nodes-yourang/issues"
  },
  "author": {
    "name": "Reddak SRL",
    "email": "matteo.schifano@reddoak.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reddoaksrl/n8n-nodes-yourang.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && ESLINT_PREPUBLISH=true npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/YourangApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Yourang/Yourang.node.js"
    ]
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^5.0.1",
    "jsonc-eslint-parser": "^2.4.2",
    "n8n": "^2.3.4",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "qs": "^6.14.1",
    "pkce-challenge": "3.1.0",
    "form-data": "^4.0.4",
    "axios": "^1.12.0",
    "semver": "^7.5.4",
    "@langchain/core": "^1.1.8",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "nodemailer": "^7.0.11",
    "@azure/identity": "^4.2.1"
  }
}
